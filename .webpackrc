{
  "extraBabelPlugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": ["dva-hmr"],
      "proxy": {
        "/api": {
          "target": "http://localhost:9000",
          "secure": false,
          "changeOrigin": true,
          "pathRewrite": {
            "^/api": ""
          }
        }
      }
    },
    "production": {
      "extraBabelPlugins": []
    }
  }
}
